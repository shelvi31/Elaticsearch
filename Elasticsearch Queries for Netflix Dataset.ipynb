 "cells": [
  {
   "cell_type": "raw",
   "id": "exempt-willow",
   "metadata": {},
   "source": [
    "#Extracting out only title\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"_source\": [\"title\"]\n",
    "}\n",
    "\n",
    "\n",
    "#Extracting out multiple stuff\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"_source\": [\"title\",\"description\",\"rating\"]\n",
    "}\n",
    "\n",
    "\n",
    "#Returning max 20 docs\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"_source\": [\"title\",\"description\",\"rating\"],\"size\":20\n",
    "}\n",
    "\n",
    "\n",
    "\n",
    "#Starting with query\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"_source\": [\"title\",\"description\",\"rating\"],\"size\":20,\n",
    "  \"query\": {\n",
    "    \"match_phrase\": {\n",
    "      \"title\": \"NORM OF THE NORTH\"          \n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "\n",
    "\n",
    "#diff btw match and match phras\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"_source\": [\"title\",\"description\",\"rating\"],\"size\":20,\n",
    "  \"query\": {\n",
    "    \"match\": {\n",
    "      \"title\": \"Norm of the north\"          \n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "\n",
    "#AGGREGATION QUERY\n",
    "\n",
    "#Finding the no of times an actor appeared\n",
    "\n",
    "#size : indicates no of o/p we want to see\n",
    "#name in aggs : bucket name we need to create\n",
    "#aggs type : type , here terms\n",
    "#field : want to do agg on which para\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"size\": 0,\n",
    "  \"aggs\": {\n",
    "    \"director-names\": {\n",
    "      \"terms\": {\n",
    "        \"field\": \"director.keyword\",\n",
    "        \"size\": 5,\n",
    "        \"order\": {\n",
    "          \"_count\": \"desc\"\n",
    "        }\n",
    "      }\n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "\n",
    "#DOING MUltiple aggregations\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"size\": 10,\n",
    "  \"query\": {\n",
    "    \"match\": {\n",
    "      \"title\": \"North\"\n",
    "    }\n",
    "  }, \n",
    "  \"aggs\": {\n",
    "    \"director-names\": {\n",
    "      \"terms\": {\n",
    "        \"field\": \"director.keyword\",\n",
    "        \"size\": 5,\n",
    "        \"order\": {\n",
    "          \"_count\": \"desc\"\n",
    "        }\n",
    "      }\n",
    "    },\n",
    "    \"country-names\": {\n",
    "      \"terms\": {\n",
    "        \"field\": \"country.keyword\",\n",
    "        \"size\": 5,\n",
    "        \"order\": {\n",
    "          \"_count\": \"desc\"\n",
    "        }\n",
    "      }\n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "\n",
    "\n",
    "#USING BOOLEAN \n",
    "\n",
    "#Diff btw must /should\n",
    "\n",
    "#must means:  logical AND.\n",
    "#should means: logical OR.\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"size\": 20,\n",
    "  \"_source\": [\"title\",\"description\", \"country\"],\n",
    "  \"query\": {\n",
    "    \"bool\": {\n",
    "      \"should\": [\n",
    "        {\n",
    "          \"match\": {\n",
    "            \"title\": \"north\"\n",
    "          }\n",
    "        },\n",
    "        {\n",
    "          \"match\": {\n",
    "            \"title\": {\n",
    "              \"query\":\"king\"\n",
    "            }\n",
    "          }\n",
    "        }\n",
    "      ]\n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "\n",
    "#Bolean query for both the words + additional\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"size\": 20,\n",
    "  \"_source\": [\"title\",\"description\", \"country\",\"director\"],\n",
    "  \"query\": {\n",
    "    \"bool\": {\n",
    "      \"filter\": [\n",
    "        {\n",
    "          \"match\": {\n",
    "            \"director\": {\n",
    "              \"query\":\"Richard\"\n",
    "            }\n",
    "          }\n",
    "        }\n",
    "        ],\n",
    "      \"must\": [\n",
    "        {\n",
    "          \"match_phrase\": {\n",
    "            \"title\": \"north\"\n",
    "          }\n",
    "        },\n",
    "        {\n",
    "          \"match_phrase\": {\n",
    "            \"title\": {\n",
    "              \"query\":\"king\"\n",
    "            }\n",
    "          }\n",
    "        }\n",
    "      ]\n",
    "    }\n",
    "  }\n",
    "}\n",
    "\n",
    "\n",
    "#Similarly we can use must_not\n",
    "\n",
    "\n",
    "#USING QUERY STRING\n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"size\": 20,\n",
    "  \"query\": {\n",
    "    \"query_string\": {\n",
    "      \"query\": \"(norm) OR (king)\",\n",
    "      \"default_field\": \"title\"\n",
    "    }\n",
    "}\n",
    "}\n",
    "\n",
    "#USING REGULAR EXP QUERY STRING\n",
    "\n",
    "# * : norm and anything coming after that \n",
    "\n",
    "GET netflix_shows/_search\n",
    "{\n",
    "  \"size\": 20,\n",
    "  \"query\": {\n",
    "    \"query_string\": {\n",
    "      \"query\": \"(norm*)\",\n",
    "      \"default_field\": \"title\"\n",
    "    }\n",
    "}\n",
    "}\n",
    "\n",
    "\n",
    "GET _cat/indices\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
  }
 ],
